{
  "name": "vouchfor-monday.com-integrations",
  "version": "1.0.0",
  "description": "A Vouchfor webhook client that integrates with Monday.com",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "PORT=4000 LOG_ENABLED=false mocha -r ts-node/register test/**/*.ts server/**/*.test.ts --exit",
    "test:debug": "npm test -- --inspect-brk",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.3",
    "pino": "^7.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.5.0",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}